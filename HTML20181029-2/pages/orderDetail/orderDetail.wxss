page{
  overflow-y:hidden;
}
input{
  height: 50rpx;
}

.container{
  width: 100%;
  height: 100%;
}

.table{
  /* height:700rpx; */
  padding:14rpx;
  position:fixed;
  top:349rpx;
  z-index:9;
  width:96%;
  
}

/* 头部导航 */
.title{
  position: fixed;
  top:100rpx;
  width: 91%;
  margin: 0 14rpx;
  height: 220rpx;
  background-color: #fff;
  border-radius: 15rpx;
  box-shadow: 0 2px 10px #dcdbdb; 
  padding: 20rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
}
 
 /* 日期时间 */
.title_date{
  color:#999999;
  font-size: 26rpx;
}

/* 地址电话 */
.title_addphone{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}

.title_t{
  width: 50%;
  height: 100%;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}

/* 导航 */
.t_tab{
  width: 114rpx;
  height: 100%;
  text-align: center;
  font-size: 28rpx;
  color: #999999;
  line-height: 100rpx;
}

/* 导航选中 */
.t_select{
  color: #ff6a00;
  border-bottom:2px solid #ff6a00; 
}

/* 滚动条 */
.scroll-v{
  width: 100%;
  height: auto;
  min-height: 440rpx;
  z-index: 1;
   margin-top: 15rpx;
  
}

/* 订单页 */
.order_v{
  height: auto;
  min-height: 420rpx;
  background-color: #fff;
  border-radius: 15rpx;
  margin-bottom: 15rpx;
  box-shadow: 0 2px 10px #dcdbdb;
  padding: 20rpx;
}

/* 订单日期 */
.order_date{
  font-size: 30rpx;
  color: #ff6a00;
}

/* 订单信息 */
.order_info{
  height: 240rpx;
  background-color: #f8f8f8;
  border-radius: 15rpx;
  margin-top: 15rpx;
  padding: 20rpx;
  display: flex;
  flex-direction: column;
}

/* 图片和名称 */
.info_imgname{
  height: 80rpx;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
}

/* 图片 */
.info_img{
  width: 80rpx;
  height: 80rpx;
  margin-right: 20rpx;
}

/* 商品名 */
.info_name{
  width: 520rpx;
  height: 80rpx;
  font-size: 30rpx;
  font-weight: bold;
  display: -webkit-box;
  word-break: break-all;
  text-overflow: ellipsis;
  overflow: hidden;
  -webkit-box-orient: vertical;
  -webkit-line-clamp:2; 
}

/* 参数行 */
.info_param{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  padding: 30rpx 0;
  border-bottom: 1rpx solid #dfdfdf;
  align-items:baseline;

}

/* 参数 */
.param_item{
  font-size: 24rpx;
  color: #999999;
  display: flex;
  flex-direction: row;
  align-items: baseline;

  height: 53rpx;
  text-align: center;
  line-height: 53rpx;
}

/* 按钮行 */
.info_btn{
  display: flex;
  flex-direction: row;
  justify-content: flex-end;
  margin-top: 20rpx;
}

/* 按钮 */
.btn_v{
  width: 100rpx;
  height: 40rpx;
  text-align: center;
  background-color: #fff;
  border: 1rpx solid #999999;
  font-size: 24rpx;
  border-radius: 20rpx;
  margin-left: 10rpx;
  line-height:40rpx;
}

/* 字体 */
.info_font{
  font-size: 30rpx;
  font-weight: bold;
}


/* 订单备注 */
.order_remark{
  min-height: 50rpx;
  background-color: #fff;
  padding: 20rpx;
  border-radius: 15rpx;
  box-shadow: 0 2px 10px #dcdbdb; 

  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}


/* 订单备注 */
.remark_name{
  font-size: 26rpx;
  width: 300rpx;
}

/* 备注文字 */
.remark_text{
  font-size: 26rpx;
  color: #999999;
  border:none;
}

/* 订单编号 */
.order_no{
  min-height: 50rpx;
  padding: 10rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 0rpx;
}

/* .bottomMsgClass{
  margin-bottom: 100rpx;
} */

/* 底部按钮 */
.bottom_btn{
  width: 100%;
  height: 100rpx;
  position: fixed;
  bottom: 0rpx;
  background-color: #fff;
  z-index: 222;
  box-shadow: 0 2px 10px #dcdbdb; 
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}

/* 按钮图片 */
.btn_back{
  width: 60rpx;
  height: 60rpx;
  margin-left: 50rpx;
}

/* 确认拒绝行 */
.yn_row{
  display: flex;
  flex-direction: row;
  z-index: 88888;
}

/* 订单总金额 */
.yesTotal{
  font-size: 34rpx;
  font-weight: bold;
  margin-left: 50rpx;
}

.total_red{
  color: #ff0000;
}

.param_yesTotal{
  font-weight: bold;
}

/*  */
.btn_yn{
  width: 50rpx;
  height: 50rpx;
  margin-right: 50rpx;
}

.yn_v{
  display: flex;
  flex-direction: column;
}

