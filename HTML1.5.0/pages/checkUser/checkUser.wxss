/* pages/checkUser/checkUser.wxss */


@import "../index/index.wxss";
page{height: 100%;}
.checkUser-image{height: 190rpx;width: 100%;position: fixed;top: 130rpx;}
.product-add{width: 96%;height: 81%;padding: 24rpx;background: #fff;border-radius: 15rpx;box-sizing: border-box;
   position: fixed;top: 242rpx;left: 2%;overflow: hidden;
 }
 .product-add-scroll{height: 100%;padding: 120rpx 56rpx 0;box-sizing: border-box;}

.checkUser-title{width: 100%;height: 110rpx;border-bottom: 1px solid #ccc;background: #fff;position: absolute;
   top: 0;left: 0;
}
.checkUser-title text{font-size: 30rpx;color: #333;font-weight: bold;display: block;float: left;width: 40%;
   padding-left: 80rpx;box-sizing: border-box;line-height: 110rpx;
}
.checkUser-title text:last-child{width: 50%;float: right;padding: 0 80rpx 0 0;box-sizing: border-box;text-align: right;}
.checkUser-name{font-size: 0;margin-bottom: 30rpx;}
.checkUser-name text{display: inline-block;vertical-align: top;font-size: 30rpx;color: #666;width: 80%;}
.checkUser-name icon{vertical-align: top;margin-top: 4rpx;}





