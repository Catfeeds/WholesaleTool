.container{
  width: 100%;
  height: 100%;
}

.table{
  height: 740rpx;
  padding: 14rpx;
  position: relative;
  top: 185rpx;
  z-index: 9;
}

/* 头部导航 */
.title{
  height: 450rpx;
  background-color: #fff;
  border-radius: 15rpx;
  box-shadow: 0 2px 10px #dcdbdb; 
  display: flex;
  flex-direction: column;
  text-align: center;
  position: relative;
  overflow: hidden;
}

.title_text{
   font-size: 30rpx;
   color: #333333;
   margin-top: 174rpx;
   font-weight: bold;
}
 
 /* 姓名 */
 .title_name{
   font-size: 30rpx;
   color: #ff6a00;
   margin-top: 80rpx;
   font-weight: bold;
 }

.title_bottom{
  position: absolute;
  bottom: 0rpx;
  width: 100%;
  height: 80rpx;
  background-color: #ff9000;
  color: #fff;
  font-size: 26rpx;
  line-height: 80rpx;
}

/* 底部菜单栏 */
.bottom_menu{
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 70rpx;
  background-color: #fff;
  display: flex;
  flex-direction: row;
  padding: 10rpx;
  padding-top: 20rpx;
  align-content: center;
}

/* 跳转按钮 */
.menu_nav{
  width: 50%;
  height: 100%;
  text-align: center;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.menu_nav>text{
  font-size: 24rpx;
  color: #9d9d9d;
}

/* 菜单图片 */
.nav_img{
  width: 40rpx;
  height: 40rpx;
}